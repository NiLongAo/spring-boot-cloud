<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<document type="freeswitch/xml">
<section name="configuration">
<configuration name="sofia.conf" description="sofia Endpoint">
<profiles>
<profile name="internal">
  <aliases>
  </aliases>
  <gateways>
  </gateways>
  <domains>
    <domain name="all" alias="true" parse="false"/>
  </domains>

  <settings>
    <param name="debug" value="0"/>
    <param name="sip-trace" value="no"/>
    <param name="sip-capture" value="no"/>
    <param name="watchdog-enabled" value="no"/>
    <param name="watchdog-step-timeout" value="30000"/>
    <param name="watchdog-event-timeout" value="30000"/>
    <param name="log-auth-failures" value="false"/>
    <param name="forward-unsolicited-mwi-notify" value="false"/>
    <param name="context" value="public"/>
    <param name="rfc2833-pt" value="101"/>
    #if($sysconf.internalPort  && $sysconf.internalPort != '')
    <param name="sip-port" value="${sysconf.internalPort}"/>
    #else
    <param name="sip-port" value="$${internal_sip_port}"/>
    #end
    <param name="dialplan" value="XML"/>
    <param name="dtmf-duration" value="2000"/>
    #if($sysconf.audioCode  && $sysconf.audioCode != '' || $sysconf.videoCode  && $sysconf.videoCode != '')
    <param name="inbound-codec-prefs" value="${sysconf.audioCode},${sysconf.videoCode}"/>
    <param name="outbound-codec-prefs" value="${sysconf.audioCode},${sysconf.videoCode}"/>
    #else
    <param name="inbound-codec-prefs" value="$${global_codec_prefs}"/>
    <param name="outbound-codec-prefs" value="$${global_codec_prefs}"/>
    #end
    <param name="rtp-timer-name" value="soft"/>
    #if($sysconf.localIp  && $sysconf.localIp != '')
        <param name="rtp-ip" value="${sysconf.localIp}"/>
        <param name="sip-ip" value="${sysconf.localIp}"/>
    #else
        <param name="rtp-ip" value="$${local_ip_v4}"/>
        <param name="sip-ip" value="$${local_ip_v4}"/>
    #end
    <param name="hold-music" value="$${hold_music}"/>
    <param name="apply-nat-acl" value="nat.auto"/>
    <param name="apply-inbound-acl" value="domains"/>
    <param name="local-network-acl" value="localnet.auto"/>
    #if($sysconf.audioRecordPath && $sysconf.audioRecordPath != '')
    <param name="record-path" value="${sysconf.audioRecordPath}"/>
    #else
    <param name="record-path" value="$${recordings_dir}"/>
    #end
    <param name="manage-presence" value="true"/>
    <param name="presence-hosts" value="$${domain},$${local_ip_v4}"/>
    <param name="presence-privacy" value="$${presence_privacy}"/>
    <param name="inbound-codec-negotiation" value="generous"/>
    <param name="tls" value="$${internal_ssl_enable}"/>
    <param name="tls-only" value="false"/>
    <param name="tls-bind-params" value="transport=tls"/>
    <param name="tls-sip-port" value="5061"/>
    <param name="tls-passphrase" value=""/>
    <param name="tls-verify-date" value="true"/>
	<param name="tls-verify-policy" value="none"/>
    <param name="tls-verify-depth" value="2"/>
    <param name="tls-verify-in-subjects" value=""/>
    <param name="tls-version" value="$${sip_tls_version}"/>
    <param name="tls-ciphers" value="$${sip_tls_ciphers}"/>
    <param name="odbc-dsn" value="pgsql-dsn::"/>
    <param name="inbound-late-negotiation" value="true"/>
    <param name="accept-blind-reg" value="false"/>
    <param name="nonce-ttl" value="60"/>
    <param name="auth-calls" value="$${internal_auth_calls}"/>
    <param name="auth-subscriptions" value="true"/>
    <param name="inbound-reg-force-matching-username" value="true"/>
    <param name="auth-all-packets" value="false"/>
    #if($sysconf.iceStart)
    <param name="ext-rtp-ip" value="${sysconf.stunAddress}"/>
    <param name="ext-sip-ip" value="${sysconf.stunAddress}"/>
    #else
    <param name="ext-rtp-ip" value="$${external_rtp_ip}"/>
    <param name="ext-sip-ip" value="$${external_sip_ip}"/>
    #end
    <param name="rtp-timeout-sec" value="300"/>
    <param name="rtp-hold-timeout-sec" value="1800"/>
    <param name="force-register-domain" value="$${domain}"/>
    <param name="force-subscription-domain" value="$${domain}"/>
    <param name="force-register-db-domain" value="$${domain}"/>
    #if($sysconf.wsPort  && $sysconf.wsPort != '')
    <param name="ws-binding"  value=":${sysconf.wsPort}"/>
    #else
    <param name="ws-binding"  value=":5066"/>
    #end
    #if($sysconf.wssPort  && $sysconf.wssPort != '')
    <param name="wss-binding" value=":${sysconf.wssPort}"/>
    #else
    <param name="wss-binding" value=":${sysconf.wssPort}"/>
    #end
    <param name="challenge-realm" value="auto_from"/>
  </settings>
</profile>
</profiles>
</configuration>
</section>
</document>
