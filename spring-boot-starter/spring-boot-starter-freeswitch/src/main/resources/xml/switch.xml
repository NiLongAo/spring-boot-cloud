<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<document type="freeswitch/xml">
  <section name="configuration" description="Various Configuration">
    <configuration name="switch.conf" description="Core Configuration">
      <cli-keybindings>
        <key name="1" value="help"/>
        <key name="2" value="status"/>
        <key name="3" value="show channels"/>
        <key name="4" value="show calls"/>
        <key name="5" value="sofia status"/>
        <key name="6" value="reloadxml"/>
        <key name="7" value="console loglevel 0"/>
        <key name="8" value="console loglevel 7"/>
        <key name="9" value="sofia status profile internal"/>
        <key name="10" value="sofia profile internal siptrace on"/>
        <key name="11" value="sofia profile internal siptrace off"/>
        <key name="12" value="version"/>
      </cli-keybindings>
      <default-ptimes>
      </default-ptimes>
      #foreach($_config in $config)
      <settings>
        <param name="colorize-console" value="true"/>
        <param name="dialplan-timestamps" value="false"/>
        <param name="max-db-handles" value="50"/>
        <param name="db-handle-timeout" value="10"/>
        <param name="max-sessions" value="1000"/>
        <param name="sessions-per-second" value="30"/>
        <param name="loglevel" value="info"/>
        <param name="mailer-app" value="sendmail"/>
        <param name="mailer-app-args" value="-t"/>
        <param name="dump-cores" value="yes"/>
        <param name="core-db-dsn" value="${_config.mysqlInTo}" />
        #if($_config.startRtpPort && $_config.startRtpPort != '')
        <param name="rtp-start-port" value="${_config.startRtpPort}"/>
        #end
        #if($_config.endRtpPort && $_config.endRtpPort != '')
        <param name="rtp-end-port" value="${_config.endRtpPort}"/>
        #end
      </settings>
      #end
    </configuration>
  </section>
</document>
